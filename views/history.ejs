                 <div class="settings">
                     <% let d =  data.depositRecords %>
                     <% if(d!=''){ %>
                        <% d.forEach(function(dt){ %>
                            <div class="settings_card nano">
                                <img class="set_img" src="/img/download.svg" alt="">
                                <div class="set_text zero">
                                    <% let type = dt.transactionHash ; if(type == null){types = 'Withdrawn to wallet'}else{types ='Deposit to wallet'}%>
                                    <h3><%= types%></h3>
                                    <% let dsp; let st = dt.approved; if(st){dsp = 'complete'}else{dsp='pending'}%>
                                    <small>status : <%= dsp%></small>
                                </div>
                                <% if(types == 'Withdrawn to wallet'){ %>
                                <div class='red'> -<%= dt.amount%></div>
                                <%}else{ %>
                                    <div class='green'> +<%= dt.amount%></div>
                                <%} %>
                            </div>
                        <% }) %>
                     <%}else{%>
                        <br><br><br><br><br>
                        <h4><%= 'no recent activity' %></h4>
                        
                     <%}%>
                        
                    </div>
               </div>
