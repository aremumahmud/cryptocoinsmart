                <div class="full">
                    <div class="part-Wrap" id="one">
                       
                        <h3 style="text-align: center;">Referals</h3><br><br>
                        <div class="kk cent">
                            <img class="set_img" src="/img/arrow-alt-circle-left.svg" alt="">
                        <img id="right" class="set_img" src="/img/arrow-alt-circle-right.svg" alt="">
                        </div>
                        <div class="settings">
                            <% let dn =  data.referalsNo %>
                            <% if(dn!='' || dn.length != 0){ %>
                               <% dn.forEach(function(dt){ %>
                                   <div class="settings_card nano">
                                       <img class="set_img" src="/img/user.svg" alt="">
                                       <div class="set_text zero">
                                           <% let type = dt.transactionHash ; if(type == null){types = dt.user}else{types = dt.user}%>
                                           <h3><%= '@'+types%></h3>
                                           <% let dsp; let st = dt.approved; if(st){dsp = dt.time}else{dsp=dt.time}%>
                                    <small>time : <%= dsp%></small>
                                       </div>
                                       <% if(types == 'Withdrawn to wallet'){ %>
                                       <div class='green'> -<%= dt.amount%></div>
                                       <%}else{ %>
                                           <div class='green'> +<%= dt.amount%></div>
                                       <%} %>
                                   </div>
                               <% }) %>
                            <%}else{%>
                               <br><br><br><br><br>
                               <h4><%= 'no referal activity' %></h4>
                               
                            <%}%>
                               
                           </div>
                 </div>
               <div class="part1-Wrap disp" id="two">
                
                <h3 style="text-align: center;">Referal Deposit Records</h3><br><br>
                <div class="kk cent">
                    <img id="left" class="set_img" src="/img/arrow-alt-circle-left.svg" alt="">
                <img  class="set_img" src="/img/arrow-alt-circle-right.svg" alt=""></div>
                 <div class="settings">
                     <% let d =  data.referalDeposits %>
                     <% if(d!=''){ %>
                        <% d.reverse().forEach(function(dt){ %>
                            <div class="settings_card nano">
                                <img class="set_img" src="/img/download.svg" alt="">
                                <div class="set_text zero">
                                    <% let type = dt.transactionHash ; if(type == null){types = 'New Referal Deposit'}else{types ='New Referal Deposit'}%>
                                    <h3><%= types%></h3>
                                    <% let dsp; let st = dt.approved; if(st){dsp = 'complete'}else{dsp='complete'}%>
                                    <small>referal : @<%= dt.user%></small>
                                </div>
                                <% if(types == 'Withdrawn to wallet'){ %>
                                <div class='red'> -<%= dt.amount%></div>
                                <%}else{ %>
                                    <div class='green'> +<%= dt.amount%></div>
                                <%} %>
                            </div>
                        <% }) %>
                     <%}else{%>
                        <br><br><br><br><br>
                        <h4><%= 'no referal deposit activity' %></h4>
                        
                     <%}%>
                        
                    </div>
               </div>
